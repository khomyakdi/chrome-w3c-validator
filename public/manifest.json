{
  "manifest_version": 3,
  "name": "W3C Validator",
  "description": "This extension will do w3c validation",
  "version": "1.0",
  "devtools_page": "devtools.html",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "file:///*"
  ]
}